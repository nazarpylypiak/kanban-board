{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "NX Serve Many (exclude analytics/notification)",
      "type": "shell",
      "command": "npx",
      "args": [
        "nx",
        "run-many",
        "-t",
        "serve",
        "--exclude=apps/backend-analytics,apps/backend-notification"
      ],
      "options": {
        "env": {
          "NX_TASKS_RUNNER_OUTPUT_STYLE": "dynamic"
        }
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [],
      "presentation": {
        "reveal": "always",
        "panel": "shared"
      }
    }
  ]
}
