{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug backend-auth with Nx",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["nx", "serve", "backend-auth"],
      "env": {
        "NODE_OPTIONS": "--inspect=9229"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/apps/backend-auth/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    },
